<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical"
    tools:context="com.chanxa.wnb.activity.staff.StaffMainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:background="@drawable/bg_navi"
        android:layout_height="@dimen/titleHeight">
        <ImageView
            android:layout_centerVertical="true"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:src="@drawable/bg_title_wnb"
            android:layout_height="wrap_content" />
    </RelativeLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/container_staff_userData"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_userdata"
                android:layout_width="70dp"
                android:layout_height="70dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:text="个人信息"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_staff_have_money_query"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_have_money_query"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="余额查询"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_staff_deal_record"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_deal_record"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="交易记录"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/container_staff_mention_now"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_mention_now"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="申请提现"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_staff_upLoadGoods"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_upload_goods"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="商品管理"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_staff_manage_order"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_manage_order"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="订单管理"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/container_staff_store_maintenance"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:src="@drawable/staff_store_maintenance"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="店面维护"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_staff_upLoadGoods2"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible">

            <ImageView
                android:src="@drawable/staff_upload_goods"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="商品管理"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/container_staff_manager_deal3"
            android:orientation="vertical"
            android:gravity="center"
            android:padding="10dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible">

            <ImageView
                android:src="@drawable/staff_manage_order"
                android:layout_width="70dp"
                android:layout_height="70dp" />

            <TextView
                android:layout_width="wrap_content"
                android:text="订单管理"
                android:textColor="@android:color/black"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

